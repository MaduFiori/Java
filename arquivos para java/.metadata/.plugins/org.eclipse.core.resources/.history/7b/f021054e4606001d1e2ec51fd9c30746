package poo;

import java.util.Random;

public class Carros {
	
	int ano;
	String cor;
	
	public Carros() {
		Random gerador = new Random();
		int chassi = gerador.nextInt(1000);
		System.out.println("Chassi: " + chassi);
	}
	
	void ligar() {
		System.out.println("ON");
	}
	
	void desligar() {
		System.out.println("OFF"); 
	}
	
	void acelerar() {
		System.out.println("VRUMMM");
	}
}
