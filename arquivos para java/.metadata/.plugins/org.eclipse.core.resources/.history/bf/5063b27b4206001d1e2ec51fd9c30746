package poo;

/**
 * @author Maria Fiori
 *
 */
public class Veiculos {

	public static void main(String[] args) {
		Carros Jeep = new Carros();
		Jeep.ano = 2020;
		Jeep.cor = "Branco";
		System.out.println("Carro Jeep");
		System.out.println("Ano: " + Jeep.ano);
		System.out.println("Cor: " + Jeep.cor);
		Jeep.ligar();
		Jeep.acelerar();
		Jeep.desligar();
	}

}
